    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cathaycineplexes.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0055A5"
      },
      "package": "com.cathaycineplexes.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}